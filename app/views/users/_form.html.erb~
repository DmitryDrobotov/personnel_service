<%= form_for @user do |f| %>
  <%= f.error_messages %>
  <div class="clearfix">
    <%= f.label :email %>
    <div class="input">
      <%= f.text_field :email %>
    </div>
  </div>
  
  <div class="clearfix">
    <%= f.label :first_name %>
    <div class="input">
      <%= f.text_field :first_name %>
    </div>
  </div>
  
  <div class="clearfix">
    <%= f.label :last_name %>
    <div class="input">
      <%= f.text_field :last_name %>
    </div>
  </div>
  
  <div class="clearfix">
    <%= f.label :position_id %>
    <div class="input">
      <%= f.number_field :position_id %>
    </div>
  </div>
  
  <div class="clearfix">
    <%= f.label :chief_id %>
    <div class="input">
      <%= f.number_field :chief_id %>
    </div>
  </div>
  
  <div class="clearfix">
    <%= f.label :role %>
    <div class="input">
      <%= f.text_field :role %>
    </div>
  </div>
  
  <p><%= f.submit %></p>
<% end %>